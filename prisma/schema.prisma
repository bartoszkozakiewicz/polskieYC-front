model User {
  id              Int       @id @default(autoincrement())
  name            String?
  surname         String?
  linkedin        String?
  scholar         String?
  introduction    String?
  accomplishments String?
  education       String?
  email           String    @unique
  hasAccess       Boolean   @default(false)
  credentials     Int       @default(0)
  createdAt       DateTime?
  lastLogin       DateTime?
  localId         String?   @unique
  customerId      String?   @unique
  priceId         String?
}
